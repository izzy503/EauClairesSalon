<!DOCTYPE html>
<html>

<head>
  <meta name="viewport" content="width=device-width" />
  <title>@ViewData["Title"] - Eau Claire's Salon</title>
  <link rel="stylesheet" href="~/css/site.css" />
  <!-- Include any additional stylesheets or scripts here -->
</head>

<body>
  <header>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <div class="container">
        <a class="navbar-brand" href="/">Eau Claire's Salon</a>
        <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav"
          aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarNav">
          <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <a class="nav-link" href="/">Home</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/Stylists/Index">Stylists</a>
            </li>
            <li class="nav-item">
              <a class="nav-link" href="/Clients/Index">Clients</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
  </header>

  <main role="main" class="container">
    @RenderBody()
  </main>

  <footer class="footer mt-auto py-3">
    <div class="container">
      <span>&copy; @DateTime.Now.Year Eau Claire's Salon</span>
    </div>
  </footer>

  <!-- Include any JavaScript files here, typically at the end of the body -->
  <script src="~/js/site.js"></script>
</body>

</html>
