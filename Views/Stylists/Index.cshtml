@model List<Stylist>

<h1>Stylists</h1>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Specialty</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    @foreach (var stylist in Model)
    {
      <tr>
        <td>@stylist.Name</td>
        <td>@stylist.Specialty</td>
        <td>
          <a asp-action="Details" asp-route-id="@stylist.StylistId">Details</a> |
          <a asp-action="Edit" asp-route-id="@stylist.StylistId">Edit</a> |
          <a asp-action="Delete" asp-route-id="@stylist.StylistId">Delete</a>
        </td>
      </tr>
    }
  </tbody>
</table>
